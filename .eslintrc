{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "indent": [ "error", 2, { "SwitchCase": 1 } ],
    //"indent":"off",
    "quotes": [ 0, "single" ],
    "semi": 0,
    "prop-types": 0,
    "no-console": 0,
    "no-trailing-spaces": 1,
    //"strict": [ 2, "global" ]
    "no-inner-declarations": "off"
  }
}